    <item label="web">
      <action name="Execute">
        <execute>qutebrowser</execute>
      </action>
    </item>
    <item label="files">
      <action name="Execute">
        <execute>thunar</execute>
      </action>
    </item>
    <item label="edit">
      <action name="Execute">
        <execute>retext</execute>
      </action>
    </item>
    <separator/>
    <menu id="applications" label="apps" execute="xdg_menu --format openbox3-pipe --root-menu /etc/xdg/menus/arch-applications.menu"/>
    <menu id="root-menu-settings" label="settings">
      <item label="obconf">
        <action name="Execute">
          <execute>obconf</execute>
        </action>
      </item>
      <item label="obkey">
        <action name="Execute">
          <command>obkey</command>
        </action>
      </item>
      <item label="obmenu">
        <action name="Execute">
          <execute>obmenu3</execute>
        </action>
      </item>
      <item label="gtktheme">
