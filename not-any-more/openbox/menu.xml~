        <action name="Execute">
          <command>lxappearance</command>
        </action>
      </item>
      <item label="panel">
        <action name="Execute">
          <execute>tint2conf</execute>
        </action>
      </item>
    </menu>
    <separator/>
    <menu id="root-menu-logout" label="bye">
      <item label="refresh">
        <action name="Execute">
          <execute>openbox --exit</execute>
        </action>
      </item>
      <item label="reboot">
        <action name="Execute">
          <execute>sudo reboot</execute>
        </action>
      </item>
      <item label="shutdown">
        <action name="Execute">
          <execute>sudo poweroff</execute>
        </action>
      </item>
    </menu>
